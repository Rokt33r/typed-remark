{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "allowJs": false,
    "jsx": "preserve",
    "baseUrl": "src",
    "rootDir": "src",
    "moduleResolution": "node",
    "inlineSourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "pretty": true
  }
}
