{
  "name": "typed-unified",
  "version": "0.1.2",
  "description": "",
  "main": "node/lib/index.js",
  "module": "es/lib/index.js",
  "types": "typings/lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:es && npm run build:node",
    "build:es": "tsc -p . -d --declarationDir typings --outDir es",
    "build:node": "tsc -p . -m commonjs --outDir node",
    "build:watch": "tsc -p . -w --outDir es",
    "test": "jest -c jest.json",
    "test:watch": "jest -c jest.json --watch es/*",
    "test:quick": "jest -c jest.json es/*",
    "lint": "tslint -c ../../tslint.json -p ./tsconfig.json",
    "clean": "rimraf es node typings",
    "prepublishOnly": "npm run lint && npm run build && npm run test"
  },
  "keywords": [],
  "author": "Junyoung Choi <fluke8259@gmail.com> (https://rokt33r.github.io/)",
  "license": "ISC",
  "devDependencies": {
    "@types/extend": "^2.0.30",
    "@types/jest": "^20.0.8",
    "@types/lodash": "^4.14.74",
    "@types/lodash.isfunction": "^3.0.3",
    "@types/node": "^8.0.28",
    "babel-jest": "^21.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "jest": "^21.1.0",
    "source-map-support": "^0.4.18",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "extend": "^3.0.1",
    "lodash": "^4.17.4",
    "lodash.isfunction": "^3.0.8",
    "typed-unist": "^0.1.0",
    "typed-vfile": "^0.1.0"
  },
  "files": [
    "src/lib/*",
    "es/lib/*",
    "node/lib/*",
    "typings/lib/*"
  ]
}
